<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <configSections>
    <section name="dictionaryConfiguration" type="Nerdle.AutoConfig.Section, Nerdle.AutoConfig" />
    <section name="complexDictionaryConfiguration" type="Nerdle.AutoConfig.Section, Nerdle.AutoConfig" />
  </configSections>

  <dictionaryConfiguration>
    <frenchWords>
      <word key="cat" value="chat" />
      <word key="dog" value="chien" />
      <word key="monkey" value="singe" />
    </frenchWords>
  </dictionaryConfiguration>

  <complexDictionaryConfiguration>
    <endpoints>
      <endPoint>
        <key>Primary</key>
        <value>
          <endpointConfiguration name="Foo" port="1" />
          <endpointConfiguration name="Bar" port="2" />
        </value>
      </endPoint>
      <endPoint>
        <key>Secondary</key>
        <value>
          <dndpointConfiguration name="Baz" port="3" />
        </value>
      </endPoint>
    </endpoints>
  </complexDictionaryConfiguration>

</configuration>